{
  "name": "figma-svelte-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "npx patch-package",
    "dev": "vite",
    "build": "rm -rf dist && npm run build:svelte && npm run build:figma",
    "build:svelte": "vite build -c vite.svelte.config.ts",
    "build:figma": "vite build -c vite.figma.config.ts",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.svelte.json"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.84.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tsconfig/svelte": "^5.0.2",
    "@types/jsdom": "^21.1.6",
    "autoprefixer": "^10.4.17",
    "figma-plugin-ds-svelte": "github:naftalibeder/figma-plugin-ds-svelte#prod",
    "js-convert-case": "4.2.0",
    "jsdom": "^23.2.0",
    "jszip": "3.10.1",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.33",
    "svelte-check": "^3.6.2",
    "svelte": "^4.2.8",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
